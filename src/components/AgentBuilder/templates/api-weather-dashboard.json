        "targetNodeId": "api-request",
        "targetPortId": "url"
      },
      {
        "id": "conn-4",
        "sourceNodeId": "api-request",
        "sourcePortId": "response",
        "targetNodeId": "json-parse",
        "targetPortId": "input"
      },
      {
        "id": "conn-5",
        "sourceNodeId": "json-parse",
