        "type": "static-text",
        "name": "API Config",
        "position": {
          "x": -400,
          "y": 150
        },
        "data": {
          "label": "API Key",
          "text": "demo_api_key_12345"
        },
        "inputs": [],
        "outputs": [
          {
            "id": "text",
            "name": "Text Output",
            "type": "output",
            "dataType": "string"
          }
        ]
      },
      {
        "id": "combine-url",
        "type": "combine-text",
        "name": "Build API URL",
        "position": {
          "x": -50,
          "y": 50
        },
        "data": {
          "label": "Build URL",
          "combineMode": "separator",
          "separator": "",
          "addSpaces": false,
          "inputs": [
            {
              "id": "text1",
              "name": "Text 1",
              "type": "input",
              "dataType": "string"
            },
            {
              "id": "text2",
              "name": "Text 2",
              "type": "input",
              "dataType": "string"
            }
          ],
          "outputs": [
            {
              "id": "combined",
              "name": "Combined Text",
              "type": "output",
              "dataType": "string"
            }
          ]
        }
      },
      {
        "id": "static-base-url",
