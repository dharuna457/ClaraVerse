        "name": "Base URL",
        "position": {
          "x": -400,
          "y": -150
        },
        "data": {
          "label": "API Base URL",
          "text": "https://api.openweathermap.org/data/2.5/weather?q=",
          "outputs": [
            {
              "id": "text",
              "name": "Text Output",
              "type": "output",
              "dataType": "string"
            }
          ]
        }
      },
