        "id": "api-request",
        "type": "api-request",
        "name": "Fetch Weather",
        "position": {
          "x": 300,
          "y": 0
        },
        "data": {
          "label": "Weather API",
          "method": "GET",
          "timeout": 10000,
          "retryCount": 3,
          "inputs": [
            {
              "id": "url",
              "name": "URL",
              "type": "input",
              "dataType": "string",
              "required": true
            }
          ],
          "outputs": [
            {
              "id": "response",
              "name": "Response",
              "type": "output",
              "dataType": "object"
            },
            {
              "id": "status",
              "name": "Status Code",
              "type": "output",
              "dataType": "number"
            }
          ]
        }
      },
      {
        "id": "json-parse",
        "type": "json-parse",
        "name": "Extract Data",
        "position": {
          "x": 650,
          "y": 0
        },
        "data": {
          "label": "Parse JSON",
          "extractField": "main.temp",
          "failOnError": false,
          "inputs": [
            {
              "id": "input",
              "name": "JSON Input",
              "type": "input",
              "dataType": "string"
            }
          ],
          "outputs": [
            {
              "id": "output",
              "name": "Parsed Data",
              "type": "output",
              "dataType": "object"
            }
          ]
        }
      },
      {
        "id": "format-report",
        "type": "llm",
        "name": "Format Report",
        "position": {
          "x": 1000,
          "y": 0
        },
        "data": {
          "label": "Weather Report",
          "apiBaseUrl": "http://localhost:8091/v1",
          "model": "gpt-3.5-turbo",
          "temperature": 0.7,
          "maxTokens": 500,
          "inputs": [
            {
              "id": "user",
              "name": "User Message",
              "type": "input",
              "dataType": "string"
            },
            {
              "id": "system",
              "name": "System Message",
              "type": "input",
              "dataType": "string"
            }
          ],
          "outputs": [
            {
              "id": "response",
              "name": "Response",
              "type": "output",
              "dataType": "string"
            }
          ]
        }
      },
      {
        "id": "system-prompt",
        "type": "static-text",
        "name": "System Prompt",
        "position": {
          "x": 650,
          "y": 200
        },
        "data": {
          "label": "Formatting Instructions",
          "text": "You are a weather report formatter. Create a friendly, readable weather summary from the JSON data provided. Include temperature, conditions, humidity, and wind speed. Keep it concise and user-friendly.",
          "outputs": [
            {
              "id": "text",
              "name": "Text Output",
              "type": "output",
              "dataType": "string"
            }
          ]
        }
      },
      {
        "id": "output-report",
        "type": "output",
        "name": "Weather Report",
        "position": {
          "x": 1350,
          "y": 0
        },
        "data": {
          "label": "Final Report",
          "format": "text",
          "inputs": [
            {
              "id": "input",
              "name": "Value",
              "type": "input",
              "dataType": "any"
            }
          ],
          "outputs": []
        }
      }
